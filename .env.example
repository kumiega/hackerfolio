# App
PUBLIC_BASE_URL=https://hackerfolio.com
PUBLIC_BASE_DOMAIN=hackerfolio.com

# Supabase URLs
PUBLIC_SUPABASE_URL=https://hackerfolio.com/supabase
PUBLIC_SUPABASE_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
SUPABASE_INTERNAL_URL=http://supabase-kong:8000

# Supabase Database
POSTGRES_PASSWORD=your-secure-db-password-here

# JWT Secret (generate with: openssl rand -base64 32)
JWT_SECRET=your-jwt-secret-here

# Studio Settings
STUDIO_DEFAULT_ORGANIZATION=hackerfolio
STUDIO_DEFAULT_PROJECT=hackerfolio

# Supabase Auth
GOTRUE_DISABLE_SIGNUP=false
GOTRUE_EXTERNAL_HOSTS=hackerfolio.com,*.hackerfolio.com
GOTRUE_URI_ALLOW_LIST=https://hackerfolio.com/*,https://*.hackerfolio.com/*
GOTRUE_EXTERNAL_GITHUB_ENABLED=true
GOTRUE_EXTERNAL_GITHUB_CLIENT_ID=your-github-client-id
GOTRUE_EXTERNAL_GITHUB_SECRET=your-github-client-secret
GOTRUE_EXTERNAL_GITHUB_REDIRECT_URI=https://hackerfolio.com/supabase/auth/v1/callback

# AI features
OPENROUTER_API_KEY=your-openrouter-api-key
OPENROUTER_DEFAULT_MODEL=anthropic/claude-3.5-sonnet
OPENROUTER_FALLBACK_MODELS=openai/gpt-4,anthropic/claude-3-opus